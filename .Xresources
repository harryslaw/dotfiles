!
! rxvt-unicode
!

! general
URxvt*saveLine:           16384
URxvt*termName:           rxvt-256color

! appearance
URxvt*scrollBar:          false
URxvt*internalBorder:     25
URxvt*cursorBlink:        1

! fonts
URxvt*boldFont:             xft: Dina:style=Bold:pixelsize=12:antialias=False
URxvt*font:                 xft: Dina:style=Regular:pixelsize=12:antialias=False

! links
URxvt*matcher.button:       1
URxvt*perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt*launcher:             /usr/bin/firefox
URxvt*underline:            true
